#include <iostream>
using namespace std;

int main() {
   int specialNum = 0;

   specialNum = 17;

   if (/* Your solution goes here  */ (specialNum == -99) || (specialNum == 0) || (specialNum == 44) /* Your solution ends here  */) {
      cout << "Special number" << endl;
   }
   else {
      cout << "Not special number" << endl;
   }

   return 0;
}
