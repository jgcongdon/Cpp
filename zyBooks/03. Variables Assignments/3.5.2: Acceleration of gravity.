#include <iostream>
using namespace std;

int main() {
   double G            = 6.673e-11;
   double M            = 5.98e24;
   double accelGravity = 0.0;
   double distCenter   = 0.0;

   distCenter = 6.38e6;

   /* Your solution goes here  */
   accelGravity = (G * M) / (distCenter * distCenter);
   /* Your solution ends here  */

   cout << "accelGravity: " << accelGravity << endl;

   return 0;
}
