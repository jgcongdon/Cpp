#include <iostream>
using namespace std;

int main() {
   int amountToChange = 0;
   int numFives = 0;
   int numOnes  = 0;

   amountToChange = 19;
   numFives = amountToChange / 5;

   /* Your solution goes here  */
   numOnes = amountToChange % 5;
   /* Your solution ends here  */

   cout << "numFives: " << numFives << endl;
   cout << "numOnes: " << numOnes << endl;

   return 0;
}
