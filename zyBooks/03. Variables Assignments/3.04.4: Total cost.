#include <iostream>
using namespace std;

int main() {
   int numDrinks;
   int numTacos;
   int totalCost;

   numDrinks = 4;
   numTacos  = 6;

   /* Your solution goes here  */
   totalCost = (numDrinks * 2) + (numTacos * 3);
   /* Your solution ends here  */

   cout << "Total cost: " << totalCost << endl;

   return 0;
}
